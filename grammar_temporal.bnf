<e> ::= <prop> | <one>
<one> ::= and <e> <e> | not <e> | until <second_prop> <interval> <interval> <second_prop>
<second_prop> ::= and <second_prop> <second_prop> | not <second_prop> | <prop>
<interval> ::= <num>
<prop> ::= proposition <var> <comp> <num>
<var> ::= x1 | x2 | x3 | x4
<comp> ::= < | >
<num> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 | 0